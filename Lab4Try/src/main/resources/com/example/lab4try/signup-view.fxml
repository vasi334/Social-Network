<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.PasswordField?>
<AnchorPane prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab4try.controller.SignUpController">
    <TextField fx:id="firstNameField" layoutX="226.0" layoutY="70.0" prefHeight="26.0" prefWidth="240.0" />
    <Label layoutX="69.0" layoutY="66.0" text="First name">
        <font>
            <Font name="Roboto Light" size="20.0" />
        </font>
    </Label>
    <TextField fx:id="lastNameField" layoutX="226.0" layoutY="136.0" prefHeight="26.0" prefWidth="240.0" />
    <Label layoutX="69.0" layoutY="132.0" text="Last name">
        <font>
            <Font name="Roboto Light" size="20.0" />
        </font>
    </Label>
    <Label fx:id="passwordLabel" layoutX="69.0" layoutY="336.0" text="Password">
        <font>
            <Font name="Roboto Light" size="20.0" />
        </font>
    </Label>
    <Button fx:id="buttonSignup" layoutX="278.0" layoutY="393.0" mnemonicParsing="false" onAction="#handleSignup" prefHeight="26.0" prefWidth="68.0" text="Sign up" />
    <PasswordField fx:id="passwordField" layoutX="226.0" layoutY="335.0" prefHeight="25.0" prefWidth="240.0" />
    <Label layoutX="69.0" layoutY="272.0" prefHeight="22.0" prefWidth="92.0" text="Age">
        <font>
            <Font name="Roboto Light" size="20.0" />
        </font>
    </Label>
    <DatePicker fx:id="ageField" layoutX="226.0" layoutY="271.0" prefHeight="32.0" prefWidth="240.0" />
   <Label fx:id="message" layoutX="103.0" layoutY="44.0" prefHeight="22.0" prefWidth="396.0" />
   <TextField fx:id="emailField" layoutX="226.0" layoutY="202.0" prefHeight="26.0" prefWidth="240.0" />
   <Label layoutX="69.0" layoutY="198.0" text="Email">
      <font>
         <Font name="Roboto Light" size="20.0" />
      </font>
   </Label>
   <Label layoutX="219.0" layoutY="444.0" text="Already have an account?" />
   <Hyperlink fx:id="backToLogin" onAction="#handleLogin" layoutX="395.0" layoutY="441.0" text="Login" />
</AnchorPane>
